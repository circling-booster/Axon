{
  "name": "dive",
  "private": true,
  "version": "0.13.0",
  "type": "module",
  "main": "dist-electron/main/index.js",
  "workspaces": [
    "packages/core-js"
  ],
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:7777/"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/OpenAgentPlatform/Dive"
  },
  "scripts": {
    "dev": "vite-node scripts/download-host-deps.ts && vite --config vite.config.electron.ts",
    "dev:tauri": "vite",
    "lint": "eslint .",
    "preview": "vite preview",
    "check": "tsc -b",
    "check-i18n": "node scripts/checkI18n.js",
    "build": "tsc -b && vite build",
    "build:core-js": "npm run build -w core-js",
    "build:pkgs": "npm run build --workspaces --if-present",
    "build:electron": "tsc -b && vite-node scripts/download-host-deps.ts && vite build --config vite.config.electron.ts",
    "postinstall": "patch-package && electron-builder install-app-deps",
    "docker:build-win": "scripts/docker/build-win.sh",
    "download:windows-bin": "vite-node scripts/download-node.ts win-x64 && vite-node scripts/download-uv.ts win32 x64 && vite-node scripts/download-python-via-uv.ts win-x64",
    "download:darwin-bin": "run-s download:darwin-bin:*",
    "download:darwin-bin:x64": "vite-node scripts/download-node.ts darwin-x64 && vite-node scripts/download-uv.ts darwin x64 && vite-node scripts/download-python-via-uv.ts darwin-x64",
    "download:darwin-bin:arm64": "vite-node scripts/download-node.ts darwin-arm64 && vite-node scripts/download-uv.ts darwin arm64 && vite-node scripts/download-python-via-uv.ts darwin-arm64",
    "download:linux-bin": "vite-node scripts/download-uv.ts linux x64 && vite-node scripts/download-python-via-uv.ts linux-x64",
    "package": "npm run build && electron-builder",
    "package:windows": "npm run download:windows-bin && run-s package:windows-*",
    "package:windows-installers": "npm run build:electron && electron-builder --win nsis --x64 --publish=never",
    "package:darwin": "npm run download:darwin-bin && npm run prepare-python darwin && npm run build:electron && electron-builder --mac --arm64 --x64 --publish=never",
    "package:darwin-dmg:arm64": "npm run prepare-python darwin-arm64 && npm run build:electron && electron-builder --mac dmg --arm64 --publish=never",
    "package:darwin-dmg:x64": "npm run prepare-python darwin-x64 && npm run build:electron && electron-builder --mac dmg --x64 --publish=never",
    "package:linux": "npm run download:linux-bin && run-s package:linux-*",
    "package:linux-tar": "npm run build:electron && electron-builder --linux tar.gz --x64 --publish=never",
    "package:linux-appImage": "npm run build:electron && electron-builder --linux appimage --x64 --publish=never",
    "prepare-python": "vite-node scripts/before-package.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.57.0",
    "@aws-sdk/client-bedrock": "^3.848.0",
    "@mistralai/mistralai": "^1.5.1",
    "@modelcontextprotocol/sdk": "^1.23.0",
    "camelcase-keys": "^10.0.1",
    "chart.js": "^4.5.1",
    "clsx": "^2.1.1",
    "core-js": "workspace:*",
    "cross-spawn": "^7.0.6",
    "electron-dl": "^4.0.0",
    "electron-log": "^5.4.3",
    "electron-store": "^10.0.1",
    "electron-updater": "^6.6.2",
    "fs-extra": "^11.3.0",
    "ollama": "^0.6.3",
    "openai": "^6.15.0",
    "react-chartjs-2": "^5.3.1",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@codemirror/lang-json": "^6.0.2",
    "@codemirror/lint": "^6.8.4",
    "@eslint/js": "^9.17.0",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-tooltip": "^1.1.7",
    "@tauri-apps/api": "2.9.1",
    "@tauri-apps/plugin-autostart": "2.5.1",
    "@tauri-apps/plugin-clipboard-manager": "2.3.2",
    "@tauri-apps/plugin-deep-link": "2.4.5",
    "@tauri-apps/plugin-dialog": "^2.6.0",
    "@tauri-apps/plugin-fs": "2.4.5",
    "@tauri-apps/plugin-http": "2.5.5",
    "@tauri-apps/plugin-log": "2.8.0",
    "@tauri-apps/plugin-opener": "2.5.2",
    "@tauri-apps/plugin-os": "2.3.2",
    "@tauri-apps/plugin-process": "2.3.1",
    "@tauri-apps/plugin-store": "2.4.2",
    "@tauri-apps/plugin-updater": "2.9.0",
    "@types/cors": "^2.8.17",
    "@types/cross-spawn": "^6.0.6",
    "@types/jsonlint": "^1.6.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/katex": "^0.16.7",
    "@types/lodash": "^4.17.15",
    "@types/mime-types": "^3.0.1",
    "@types/node": "^24.1.0",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@types/tar": "^6.1.13",
    "@types/unzipper": "^0.10.11",
    "@types/which": "^3.0.4",
    "@types/ws": "^8.18.1",
    "@uiw/react-codemirror": "^4.23.8",
    "@vitejs/plugin-react": "^4.3.4",
    "clsx": "^2.1.1",
    "electron": "^39.2.7",
    "electron-builder": "^26.4.0",
    "env-paths": "^3.0.0",
    "eslint": "^9.17.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "i18next": "^25.3.0",
    "i18next-browser-languagedetector": "^8.0.2",
    "jotai": "^2.11.0",
    "jsonlint-mod": "^1.7.6",
    "katex": "^0.16.21",
    "lodash": "^4.17.21",
    "marked": "^15.0.7",
    "marked-react": "^3.0.0",
    "mermaid": "^11.10.1",
    "mitt": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "patch-package": "^8.0.0",
    "pretendard": "^1.3.9",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.6.1",
    "react-intersection-observer": "^9.16.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^6.28.1",
    "react-syntax-highlighter": "^15.6.1",
    "react-textarea-autosize": "^8.5.9",
    "rehype-katex": "^7.0.1",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.1",
    "remark-math": "^6.0.0",
    "rimraf": "^6.0.1",
    "sass-embedded": "^1.83.0",
    "ts-jest": "^29.2.5",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.38.0",
    "unzipper": "^0.12.3",
    "vite": "^6.3.5",
    "vite-node": "^3.2.4",
    "vite-plugin-electron": "^0.29.0",
    "vite-plugin-electron-renderer": "^0.14.6",
    "vitest": "^2.1.9",
    "which": "^6.0.0",
    "ws": "^8.18.3"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.9",
    "utf-8-validate": "^6.0.5"
  }
}
